<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.FXMLVisitController">
   <children>
      <VBox layoutX="129.0" layoutY="6.0" prefWidth="350.0" spacing="10.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <HBox alignment="CENTER_RIGHT" layoutX="30.0" layoutY="30.0" prefWidth="200.0" spacing="20.0">
               <children>
                  <Label text="Visit start" />
                  <DatePicker fx:id="startDatePicker" prefHeight="25.0" prefWidth="171.0" promptText="startDatePicker" />
               </children>
            </HBox>
            <HBox alignment="CENTER_RIGHT" layoutX="30.0" layoutY="30.0" prefWidth="200.0" spacing="20.0">
               <children>
                  <Label text="Visit end" />
                  <DatePicker fx:id="endDatePicker" prefHeight="25.0" prefWidth="171.0" promptText="endDatePicker" />
               </children>
            </HBox>
            <HBox alignment="CENTER_RIGHT" layoutX="30.0" layoutY="65.0" prefWidth="200.0" spacing="20.0">
               <children>
                  <Label text="Stayed in" />
                  <ChoiceBox id="stayedInChoiceBox" fx:id="inChoiceBox" prefHeight="25.0" prefWidth="169.0" />
               </children>
            </HBox>
            <Separator />
            <HBox fx:id="caravanMakeHBox" alignment="CENTER_RIGHT" layoutX="30.0" layoutY="300.0" prefWidth="200.0" spacing="20.0">
               <children>
                  <Label text="Make" />
                  <TextField id="makeTextField" fx:id="caravanMakeTextField" />
               </children>
            </HBox>
            <HBox fx:id="caravanModelHBox" alignment="CENTER_RIGHT" layoutX="30.0" layoutY="480.0" prefWidth="200.0" spacing="20.0">
               <children>
                  <Label text="Model" />
                  <TextField id="modelTextField" fx:id="caravanModelTextField" />
               </children>
            </HBox>
            <HBox fx:id="caravanLengthHBox" alignment="CENTER_RIGHT" layoutX="30.0" layoutY="445.0" prefWidth="200.0" spacing="20.0">
               <children>
                  <Label text="Dimensions" />
                  <TextField id="dimensionsTextField" fx:id="caravanLengthTextField" />
               </children>
            </HBox>
            <HBox alignment="CENTER" spacing="20.0">
               <children>
                  <CheckBox id="electricityCheckBox" fx:id="electricityCheckBox" mnemonicParsing="false" text="Electricity hookup" GridPane.columnIndex="1" />
               </children>
               <padding>
                  <Insets bottom="10.0" top="10.0" />
               </padding>
            </HBox>
            <HBox alignment="CENTER" prefWidth="200.0" spacing="20.0">
               <children>
                  <Button id="saveButton" mnemonicParsing="false" onAction="#handleSaveButton" text="Save" />
                  <Button id="cancelButton" mnemonicParsing="false" onAction="#handleCancelButton" text="Cancel" />
               </children>
            </HBox>
         </children>
         <padding>
            <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
         </padding>
      </VBox>
   </children>
</AnchorPane>
